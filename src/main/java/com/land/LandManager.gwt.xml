<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='landmanager'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="com.google.gwt.i18n.I18N"/>
	
	<replace-with class="com.land.client.ClientFactoryImpl">
		<when-type-is class="com.land.client.ClientFactory" />
	</replace-with>

	<entry-point class='com.land.client.LandManager' />
	<extend-property name="locale" values="ru" />
	<set-property name="locale" value="ru" />

	<source path='client' />
	<source path='shared' />

</module>
